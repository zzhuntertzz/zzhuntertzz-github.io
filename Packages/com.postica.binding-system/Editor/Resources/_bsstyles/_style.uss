}

.bind-flags__toggle:hover .bind-flags__toggle__initial {
    color: var(--bind-flags-hover);
}

.bind-flags__toggle:checked .bind-flags__toggle__initial {
    color: var(--bind-flags-on);
}


/* MATH EXPRESSION DRAWER */
.math-expression__label-parent {
    position: absolute;
    top: 3px;
    left: 5px;
}

.math-expression__field .unity-text-element{
    color: transparent;
}

.math-expression__info {
    width: 16px;
    height: 16px;
    margin-right: -4px;
    --unity-image: resource('_bsicons/circle_check');
    --unity-image-tint-color: var(--math-expression-valid);
}

.math-expression--error .math-expression__info {
    --unity-image: resource('_bsicons/circle_error');
    --unity-image-tint-color: var(--error-color-fg);
}

.math-expression--error .math-expression__field .unity-base-field__input {
    background-color: var(--error-fade-color-bg);
}

/* BOOL EXPRESSION DRAWER */
.bool-expression__label-parent {
    position: absolute;
    top: 3px;
    left: 5px;
}

.bool-expression__field .unity-text-element{
    color: transparent;
}

.bool-expression__info {
    width: 16px;
    height: 16px;
    margin-right: -4px;
    --unity-image: resource('_bsicons/circle_check');
    --unity-image-tint-color: var(--math-expression-valid);
}

.bool-expression--error .bool-expression__info {
    --unity-image: resource('_bsicons/circle_error');
    --unity-image-tint-color: var(--error-color-fg);
}

.bool-expression--error .bool-expression__field .unity-base-field__input {
    background-color: var(--error-fade-color-bg);
}

 /* BIND FIELD REROUTING */
.bind-data__reroute-icon {
    --unity-image: resource('_bsicons/route');
    --unity-image-tint-color: var(--bind-reroute-color);
    width: 14px;
    height: 14px;
    margin-right: 1px;
}

.bind-data__reroute-icon:hover {
    --unity-image-tint-color: var(--bind-reroute-color-hover);
}


 /* UNITY EVENT OVERRIDE */
.unity-event--proxy #unity-event__listener-target {
    display: none;
}

.unity-event--proxy #unity-event__parameter-property {
    display: none;
}

.unity-event--proxy #unity-event__object-parameter {
    display: none;
}

#unity-event__proxy-target {
    display: none;
}

#unity-event__proxy-path {
    display: none;
}

.unity-event--proxy #unity-event__proxy-target {
    display: flex;
}

.unity-event--proxy #unity-event__proxy-path {
    display: flex;
    height: 19px;
    margin: 1px 0;
    -unity-text-align: middle-left;
    padding-left: 20px;
}

.unity-event--proxy .unity-event__proxy-path__icon {
    width: 14px;
    height: 14px;
    position: absolute;
    left: 4px;
}

.unity-event__proxy-path__unbound {
    display: none;
    position: absolute;
    right: 8px;
    top: 0;
    bottom: 0;
    font-size: 10px;
    -unity-font-style: bold;
    color: var(--error-weak-color-fg);
}

.unity-event__proxy-path--unbound .unity-event__proxy-path__unbound {
    display: flex;
}