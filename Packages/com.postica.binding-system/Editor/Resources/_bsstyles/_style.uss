:root {
    --error-color-fg: #ff0000;
    --error-weak-color-fg: #f66767;
    --error-color-bg: #ff0000;
    --error-weak-color-bg: #cc0000;
    --error-weak-transparent-color-bg: rgba(250, 0, 0, 0.25);
    --error-fade-color-bg: #411f1f;
    --bind-primary-color: #7fdbef;
    --bind-primary-faint-color: #368aa3;
    --bind-primary-very-faint-color: #28484c;
    --bind-debug-color: #eeae00;
    --bind-debug-color-bg: #684c00;
    --bind-debug-invalid-color: #757575;
    --bind-type-label-color: #eeae00;
    --bind-path-button-bg: #1f1f1f;
    --bind-path-drag-show-toggle: #02414e;
    --bind-path-drag-valid-bg: #7fdbef;
    --bind-path-drag-invalid-bg: #ee2222;
    --bind-path-drag-changed-bg: #24e200;
    --bind-mode-bg: #9A9A9A;
    --bind-mode-active-bg: #7fdbef;
    --bind-dark-font: #111111;
    --bind-mode-icon: black;
    --bind-mode-disabled-icon: black;
    --bind-mode-hover: #87f5ff;
    --bind-mode-debug-hover: #ffcb00;
    --connecting-line: #757575;
    --connecting-line-hover: #10c0e7;
    --converter-safe-color: #d5d5d5;
    --converter-safe-bg: #323232;
    --converter-unsafe-color: #eeae00;
    --converter-icon-tint: #222222;
    --converter-icon-tint-unsafe: #ba8800;
    --modifier-remove-color-bg: #aa0000;
    --modifier-remove-color-hover-bg: #cc0000;
    --modifier-bg: #313131;
    --modifier-hover-bg: #2b2b30;
    --modifier-hover-border: #368aa3;
    --modifier-incompatible-fg: rgba(89, 89, 89, 1);
    --modifier-incompatible-icon-fg: #980000;
    --live-preview-image-tint: rgba(255, 255, 255, 0.75);
    --live-preview-active-image-tint: rgb(255, 194, 0);
    --common-target-description: #979797;
    --common-target-smart: #439195;
    --common-target-smart-bg: rgba(67, 145, 149, 0.25);
    --common-target-smart-hover-bg: rgb(37, 209, 218);
    --common-target-forced: #c58429;
    --common-target-forced-bg: rgba(197, 132, 41, 0.25);
    --common-target-forced-hover-bg: rgb(248, 147, 0);
    --common-target-success: #3bba1f;
    --common-target-fail: #dd4040;
    --common-target-hover-text-color: black;
    --clear-color: rgba(0, 0, 0, 0);
    --unity-button-bg: rgba(89, 89, 89, 1);
    --unity-text-color: rgba(210, 210, 210, 0.5);
    --unity-bg: #383838;
    
    --math-expression-valid: #3bba1f;
    
    --bind-preview-background: rgba(0, 0, 0, 0.15);
    --bind-preview-background-edit: rgba(197, 132, 41, 0.1);
    --bind-preview-border: rgba(117, 119, 122, 0.4);
    --bind-preview-border-edit: #eeae00;
    --bind-preview-label: rgb(111, 143, 147);
    --bind-preview-label-edit: rgba(197, 132, 41, 0.9);
    --bind-preview-edit-label: #878787;
    --bind-preview-edit-label-hover: rgba(238, 174, 0, 0.6);
    --bind-preview-edit-label-checked: #eeae00;
    
    --bind-flags-off: #b5b5b5;
    --bind-flags-on: #eeae00;
    --bind-flags-hover: rgba(238, 174, 0, 0.8);
    --bind-flags-hover-bg: rgba(0, 0, 0, 0.15);
    --bind-flags-on-bg: rgba(0, 0, 0, 0.3);
    --bind-flags-separator: rgba(107, 116, 119, 0.4);
    
    --bind-reroute-color: rgba(195, 195, 195, 0.5);
    --bind-reroute-color-hover: rgba(195, 195, 195, 0.7);
    
    --bind-focus-color: #1e1e1e;
    --bind-focus-bg-color: #eeae00;
}

.hidden {
    display: none;
}

.transparent {
    opacity: 0;
}

.horizontal {
    flex-direction: row;
}

.bs-bind-elem {
    flex-grow: 1;
    flex-shrink: 1;
}

.bs-bind-value__foldout .bs-bind-toggle--off-slot > .unity-foldout__checkmark {
    margin-right: 0px;
}

.bs-bind-value__foldout .bs-bind-toggle--off-slot > .bs-bind-toggle {
    margin-left: 2px;
    margin-right: 4px;
    margin-top: 0px;
}

.bs-bind-value.no-editor {
    flex-grow: 1;
}

.bs-bind-value.no-editor .unity-base-field__label {
    width: 35%;
}

.bs-bind-value.no-editor .unity-base-field__input {
    flex-grow: 1;
    flex-direction: row;
    align-self: center;
    justify-content: center;
    font-size: 10px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    color: var(--bind-debug-color);
    min-height: 18px;
    background-color: var(--modifier-bg);
}

    .bs-bind-value.no-editor .bs-bind-value__icon {
        --unity-image: resource('_bsicons/forbidden');
        --unity-image-tint-color: var(--error-color-fg);
        width: 12px;
        height: 12px;
        align-self: center;
        margin-right: 2px;
    }

.bs-bind {
    flex-direction: row;
}

.bs-bind.runtime-fail {
    background-color: var(--error-weak-transparent-color-bg);
    padding-top: 1px;
    padding-bottom: 4px;
    padding-right: 4px;
    margin-right: -4px;
    border-radius: 4px;
    border-width: 2px;
    border-color: var(--error-weak-color-bg);
}

.bs-bind.runtime-fail .bs-bind-proxy__background {
    background-color: var(--error-weak-transparent-color-bg);
}

.bs-bind-toggle {
    max-height: 20px;
    /* margin-left: -12px; */
}

.bs-bind-toggle .unity-toggle__input {
    flex-direction: column;
}

.bs-bind-toggle #unity-checkmark {
    top: 2px;
    background-image: resource('_bsicons/bind_off'); 
}

    .bs-bind-toggle:hover #unity-checkmark {
        top: 2px;
        background-image: resource('_bsicons/bind_off_hover');
    }

.bs-bind-toggle.bs-bind-toggle--on #unity-checkmark {
    background-image: resource('_bsicons/bind_on');
}

.bind-data-for > .bs-bind-toggle.bs-bind-toggle--on #unity-checkmark {
    -unity-background-image-tint-color: #ADADAD;
}

.bs-bind__label--hybrid {
    flex-grow: 1;
    overflow: visible;
}

    .bs-bind__label--hybrid .hybrid-label {
        position: absolute;
        right: 4px;
        top: 0px;
        bottom: 0px;
        font-size: 10px;
        -unity-text-align: middle-center;
        -unity-font-style: bold;
        color: var(--bind-debug-color-bg);
    }

    .bs-bind__label--hybrid .hybrid-icon {
        position: absolute;
        left: -11px;
        align-self: center;
        border-top-width: 1px;
        width: 6px;
        height: 2px;
        background-color: var(--unity-text-color);
    }

.bs-bind-value > .unity-base-field.unity-toggle {
    margin-bottom: 2px;
}

    .bs-bind-value > .unity-base-field.unity-toggle .bind-field__label {
        padding-top: 2px;
    }
    
/* Bind Data Generics */
.bind-data.runtime-fail {
    background-color: var(--error-weak-transparent-color-bg);
    padding-top: 1px;
    left: -20px;
    padding-left: 20px;
    padding-right: 4px;
    margin-right: -24px;
    border-radius: 4px;
    border-width: 2px;
    border-color: var(--error-weak-color-bg);
}

.error-point {
    background-color: var(--error-color-bg);
    color: black;
    width: 16px;
    height: 16px;
    padding: 0px;
    font-size: 13px;
    -unity-font-style: bold;
    border-radius: 4px;
}

.bind-data--expanded {
    padding: 4px;
    background-color: #202020;
    border-radius: 6px;
    border-width: 2px;
    border-color: var(--bind-primary-faint-color);
    transition: border-color 0.3s;
}

.bind-data--expanded.live-debug {
    border-color: var(--bind-debug-color);
}

.bind-data.slim .connecting-line__owner, .bind-data.slim .bind-modifiers {
    display: none;
}

.bind-field__label {
    transition: color 0.5s;
}

.bind-data.must-focus .bind-field__label {
    color: var(--bind-focus-color);
    transition: color 0.15s;
}

.bind-data.must-focus .unity-base-field__label {
    color: var(--bind-focus-color);
    transition: color 0.15s;
}

.bind-data.must-focus .bind-modifiers .unity-list-view__item {
    background-color: var(--bind-focus-bg-color);
    transition: background-color 0.15s;
}

.bind-data {
    border-radius: 4px;
    transition: background-color 0.2s, scale ease-out-cubic 0.2s;
}

.bind-data.must-focus {
    background-color: var(--bind-focus-bg-color);
    scale: 1.06;
    overflow: hidden;
    transition: background-color 0.2s, scale ease-out-elastic 0.8s;
}

/* Bind Data Menu */
.bind-data__menu {
    padding: 0px;
    margin: 0px 0px 0px 1px;
    width: 18px;
    align-items: center;
    justify-content: center;
}

.bind-data__menu__icon {
    width: 10px;
    height: 10px;
    --unity-image: resource('_bsicons/menu');
    --unity-image-tint-color: #b5b5b5;
}

/* Bind Data TARGET */
.bind-data__target {

}

.bind-data__target.hidden {
    display: none;
}

.bind-data__target__info {
    font-size: 10px;
    -unity-text-align: middle-center;
    -unity-font-style: bold;
    color: var(--bind-primary-color);
}

.bind-data__target__info.as-error {
    color: var(--bind-debug-color);
}

.bind-data__target .error-point {
    display: none;
}

    .bind-data__target.has-error .error-point {
        display: flex;
    }

.bind-data__target.has-error .unity-base-field__label {
    color: var(--error-color-fg);
}

    .bind-data__target.has-error .bind-data__target__info {
        color: var(--error-color-fg);
    }

.bind-data__target.has-error .unity-object-field__input {
    border-color: var(--error-color-fg);
}

.bind-data .enhanced-foldout__header-start {
    min-width: 30%;
    flex-shrink: 1;
}

/* Bind Data PATH VIEW */

.bind-data__path {
    flex-direction: row;
    margin-bottom: 1px;
    margin-top: 0;
}

/*.bind-data__path.minimal .unity-button, .bind-data__path.minimal .bind-data__path__button {
    background-color: var(--bind-path-button-bg);
}*/

.bind-data__path.has-error .bind-data__path__label {
    color: var(--error-color-fg);
}

    .bind-data__path .error-point {
        display: none;
    }

    .bind-data__path.has-error .error-point {
        display: flex;
    }

.bind-data__path__type {
    width: 16px;
    height: 16px;
}

/* Bind Debug */

.bind-data__path__debug{
    width: 16px;
    height: 16px;
    margin: 0px 3px;
}

    .bind-data__path__debug #unity-checkmark {
        width: 16px;
        height: 16px;
        background-image: resource('_bsicons/debug_empty');
        -unity-background-image-tint-color: rgba(255, 255, 255, 0.3);
        transition: -unity-background-image-tint-color 0.2s;
    }

    .bind-data__path__debug:hover #unity-checkmark {
        background-image: resource('_bsicons/debug_empty');
        -unity-background-image-tint-color: rgba(238, 174, 0, 1);
    }

    .bind-data__path__debug:checked #unity-checkmark {
        background-image: resource('_bsicons/debug');
        -unity-background-image-tint-color: rgba(238, 174, 0, 0.8);
    }

    .bind-data__path__debug:checked:hover #unity-checkmark {
        background-image: resource('_bsicons/debug');
        -unity-background-image-tint-color: rgba(238, 174, 0, 1);
    }

/* Bind Mode */

.bind-data__path__mode {
    margin: 0px;
    padding: 0px 3px;
    margin-left: 2px;
    border-right-width: 0px;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    
    transition: background-color 0.3s;
}

.bind-data__path.minimal .enhanced-foldout__header-rest {
    margin-left: -25px;
}

.bind-data.slim .bind-data__path.minimal .enhanced-foldout__header-rest {
    justify-content: flex-end;
    margin-right: 8px;
}

.bind-data--no-label .bind-data__path.minimal .enhanced-foldout__header-rest {
    margin-left: -6px;
}

.bind-data__path.minimal .bind-data__path__mode {
    background-color: var(--bind-mode-bg);
}

    .bind-data__path.minimal .bind-data__path__mode.can-change-value {
        background-color: var(--bind-mode-active-bg);
    }

    .bind-data__path.minimal .bind-data__path__mode.can-change-value:hover {
        background-color: var(--bind-mode-hover);
    }

.bind-data__path.minimal.live-debug .bind-data__path__mode {
    background-color: var(--bind-debug-color);
}

.bind-data__path.minimal.live-debug .bind-data__path__mode.can-change-value:hover {
    background-color: var(--bind-mode-debug-hover);
}

.bind-data__path.minimal .bind-data__path__mode.long-clicking {
    background-color: var(--bind-debug-color);
}

.bind-data__path__mode .bind-data__path__mode__icon {
    height: 16px;
    width: 16px;
    --unity-image-tint-color: var(--bind-primary-color);
}

.bind-data__path.minimal .bind-data__path__mode .bind-data__path__mode__icon {
    --unity-image-tint-color: var(--bind-mode-disabled-icon);
}

.bind-data__path.minimal .bind-data__path__mode.can-change-value .bind-data__path__mode__icon {
    --unity-image-tint-color: var(--bind-mode-icon);
}

.bind-data__path.minimal .bind-data__path__mode.live-debug-reveal .bind-data__path__mode__icon {
    height: 14px;
    width: 14px;
    margin: 1px;
    --unity-image: resource('_bsicons/debug');
}

    .bind-data__path__mode.bind-mode--read .bind-data__path__mode__icon {
        --unity-image: resource('_bsicons/read');
    }

    .bind-data__path__mode.bind-mode--write .bind-data__path__mode__icon {
        --unity-image: resource('_bsicons/write');
    }

    .bind-data__path__mode.bind-mode--write.bind-mode--read .bind-data__path__mode__icon {
        --unity-image: resource('_bsicons/readwrite');
    }

    .bind-data__path__mode.multiple-modes .bind-data__path__mode__icon {
        --unity-image: resource('_bsicons/question_sign');
    }


.bind-data__path.has-error .bind-data__path__mode.has-error {
    background-color: var(--error-color-bg);
    border-color: var(--error-color-fg);
}

/* Bind Path Compact Toggles */

.bind-data__path__show-toggle {
    margin: 0px;
    padding: 0px 1px;
    border-radius: 0px;
    border-width: 1px 0px 1px 1px;
    min-width: auto;
    transition: background-color 0.5s, border-color 0.5s;
}

.bind-data__path__show-toggle .unity-toggle__checkmark {
    display: none;
}

.bind-data__path__show-toggle.target-changed {
    background-color: var(--bind-path-drag-changed-bg);
    border-color: var(--bind-path-drag-changed-bg);
}

.bind-data__path__show-toggle.has-error {
    background-color: var(--error-weak-color-bg);
    border-color: var(--error-color-fg);
}

    .bind-data__path__show-toggle.has-error:checked {
        background-color: var(--unity-button-bg);
    }

.bind-data__path__show-toggle__icon {
    width: 16px;
    height: 16px;
    --unity-image-tint-color: white;
}

.bind-data__path__show-toggle--modifiers .bind-data__path__show-toggle__icon {
    --unity-image: resource('_bsicons/modify');
    --unity-image-tint-color: var(--live-preview-image-tint);
}

.bind-data__path__show-toggle--modifiers:checked .bind-data__path__show-toggle__icon {
    --unity-image-tint-color: var(--live-preview-active-image-tint);
}

.bind-data__path__show-toggle--converters .bind-data__path__show-toggle__icon {
    --unity-image: resource('_bsicons/convert');
    --unity-image-tint-color: var(--converter-safe-color);
}

.bind-data__path__show-toggle--converters.unsafe .bind-data__path__show-toggle__icon {
    --unity-image: resource('_bsicons/convert');
    --unity-image-tint-color: var(--converter-unsafe-color);
}

.bind-data__path__show-toggle--preview .bind-data__path__show-toggle__icon {
    width: 14px;
    height: 14px;
    margin: 1px;
    --unity-image-tint-color: var(--live-preview-image-tint);
    --unity-image: resource('_bsicons/lens');
}

.bind-data__path__show-toggle--preview:checked .bind-data__path__show-toggle__icon {
    --unity-image-tint-color: var(--live-preview-active-image-tint);
}

/*.bind-data__path__show-toggle--converters.unsafe.hidden {
    display: flex;
}*/

.bind-data__path__show-toggle--target.ping-object {
    background-color: var(--bind-debug-color-bg);
}

.bind-data__path__show-toggle--target .bind-data__path__show-toggle__icon.null-object {
    --unity-image: resource('_bsicons/expand');
    --unity-image-tint-color: var(--bind-primary-color);
}

.bind-data__path__show-toggle--target .bind-data__path__show-toggle__icon.multiple-types {
    --unity-image: resource('_bsicons/question_mark');
    --unity-image-tint-color: var(--bind-debug-color);
}

.bind-data__path__show-toggle--target .bind-data__path__show-toggle__icon.incompatible-types {
    --unity-image: resource('_bsicons/question_mark');
    --unity-image-tint-color: var(--error-color-fg);
}

.bind-data__path__show-toggle__label {
    position: absolute;
    right: -2px;
    bottom: -3px;
    padding: 0px 1px;
    font-size: 10px;
    border-radius: 4px;
    /*width: 10px;*/
    background-color: black;
    color: var(--bind-debug-color);
}

/* EXPAND VIEW Button */
.bind-data__path__expand {
    display: none;
}

.bind-data.slim .bind-data__path__expand {
    display: flex;
    margin: -1px 0 0 0;
    height: 20px;
    width: 60px;
    font-size: 10px;
    -unity-font-style: bold;
    flex-shrink: 0;
    align-self: flex-end;
    flex-direction: row;
    justify-content: center;
    right: 0;
    transition: background-color 0.3s;
}

.bind-data__path__expand__icon {
    width: 14px;
    height: 14px;
    margin-right: 2px;
    --unity-image: resource('_bsicons/reveal');
    --unity-image-tint-color: var(--bind-primary-color);
}

.bind-data__path__expand__label {
    color: var(--bind-primary-color);
    transition: color 0.3s;
}

.bind-data__path.live-debug .bind-data__path__expand__icon {
    --unity-image-tint-color: var(--bind-debug-color);
}

.bind-data__path.live-debug .bind-data__path__expand__label {
    color: var(--bind-debug-color);
}

.popup-window-source .bind-data__path__expand{
    background-color: var(--bind-primary-color);
}

.popup-window-source .bind-data__path__expand__icon{
    --unity-image-tint-color: var(--bind-mode-icon);
}

.popup-window-source .bind-data__path__expand__label{
    color: var(--bind-mode-icon);
}

.popup-window-source .bind-data__path.live-debug .bind-data__path__expand{
    background-color: var(--bind-debug-color);
}

.popup-window-source .bind-data__path.live-debug .bind-data__path__expand__icon{
    --unity-image-tint-color: var(--bind-mode-icon);
}

.popup-window-source .bind-data__path.live-debug .bind-data__path__expand__label{
    color: var(--bind-mode-icon);
}

/* Bind Path */
.bind-data.slim .bind-data__path-dropdown {
    display: none;
}

.bind-data__path-dropdown {
    flex-direction: row;
    flex-basis: 100%;
    /*flex-grow: 1;*/
}

.bind-data__path__button {
    margin-left: 0px;
    border-top-left-radius: 0px;
    border-bottom-left-radius: 0px;
    padding-right: 16px;
    flex-grow: 0;
    flex-shrink: 1;
    flex-basis: 100%;
    /*transition: background-color 0.1s, border-color 0.1s;*/
}

    .bind-data__path__button .dropdown-value {
        overflow: hidden;
        text-overflow: ellipsis;
        -unity-text-overflow-position: start;
        white-space: nowrap;
        flex-grow: 1;
        flex-shrink: 0;
    }

    .bind-data__path.path-not-set .dropdown-value {
        font-size: 9px;
        align-self: center;
        -unity-font-style: bold;
        color: var(--bind-primary-color);
        margin-right: 6px;
    }

.bind-data__path.target-set .dropdown-value {
    font-size: 9px;
}

.bind-data__path__type-label {
    flex-grow: 0;
    flex-shrink: 1;
    font-size: 9px;
    overflow: hidden;
    margin-left: 4px;
    -unity-font-style: bold;
    color: var(--bind-type-label-color);
}

.bind-data__path .unity-base-popup-field__arrow {
    position: absolute;
    right: 4px;
}

.bind-data__path.has-error .bind-data__path__button {
    border-color: var(--error-color-fg);
}

.bind-data__path__drag-label {
    position: absolute;
    right: 0px;
    top: 0px;
    bottom: 0px;
    left: 0px;
    -unity-text-align: middle-center;
    -unity-font-style: bold;
    color: var(--bind-dark-font);
    font-size: 10px;
    display: none;
}

    /*.bind-data__path__drag-label.valid {
        background-color: var(--bind-path-drag-valid-bg);
    }

    .bind-data__path__drag-label.invalid {
        background-color: var(--bind-path-drag-invalid-bg);
    }*/

    .bind-data__path-dropdown.dnd-valid .bind-data__path__button {
        border-color: var(--bind-path-drag-valid-bg);
        background-color: var(--bind-path-drag-valid-bg);
    }

.bind-data__path-dropdown.dnd-valid .bind-data__path__show-toggle--target {
    border-color: var(--bind-path-drag-valid-bg);
    background-color: var(--bind-path-drag-show-toggle);
}

.bind-data__path-dropdown.dnd-valid .bind-data__path__drag-label.valid {
    display: flex;
}

.bind-data__path-dropdown.dnd-valid .bind-data__path__mode {
    border-color: var(--bind-path-drag-valid-bg);
}

.bind-data__path-dropdown.dnd-valid .bind-data__path__button .unity-label {
    display: none;
}

