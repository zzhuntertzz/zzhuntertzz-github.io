/* Common Target */
.unity-decorator-drawers-container > .unity-header-drawer__label {
    margin-top: 9px;
    min-height: 20px;
}

.common-target {
    flex-direction: row;
    flex-grow: 1;
    flex-shrink: 0;
    justify-content: flex-end;
    align-items: center;
    transition: opacity 0.3s;
}

.common-target.invisible {
    width: 0;
}

.common-target.inactive {
    opacity: 0;
    visibility: hidden;
}

.common-target .tooltip-blocker {
    display: none;
}

.common-target.inactive .tooltip-blocker {
    display: flex;
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
}

.unity-list-view__foldout-header .common-target {
    margin-right: 52px;
}

.unity-header-drawer__label .common-target {
    margin-right: -4px;
}

.common-target__description {
    font-size: 11px;
    padding: 0px;
    -unity-text-align: middle-center;
    -unity-font-style: bold;
    color: var(--common-target-description);
    margin-right: 4px;
}

.unity-header-drawer__label .common-target__description{
    -unity-text-align: lower-center;
    align-self: flex-end;
}

.common-target__zone {
    margin: 0px 1px;
    padding: 2px 8px;
    font-size: 11px;
    border-width: 1px;
    border-radius: 4px;
    -unity-font-style: bold;
    background-image: resource("_bsicons/lines");
    background-repeat: repeat;
    background-size: contain;
}

.common-target__zone.smart {
    -unity-background-image-tint-color: var(--common-target-smart-bg);
    color: var(--common-target-smart);
    border-color: var(--common-target-smart);
}

.common-target__zone.smart.dnd-valid {
    background-color: var(--common-target-smart-hover-bg);
    color: var(--common-target-hover-text-color);
}

.common-target.dnd-valid .common-target__zone.smart {
    background-color: var(--common-target-smart-hover-bg);
    color: var(--common-target-hover-text-color);
}

.common-target__zone.forced {
    -unity-background-image-tint-color: var(--common-target-forced-bg);
    color: var(--common-target-forced);
    border-color: var(--common-target-forced);
}

.common-target__zone.forced.dnd-valid {
    background-color: var(--common-target-forced-hover-bg);
    color: var(--common-target-hover-text-color);
}

.common-target__zone.dnd-succeeded {
    background-image: none;
    background-color: var(--common-target-success);
    border-color: var(--common-target-success);
    color: var(--common-target-hover-text-color);
}

.common-target__zone.dnd-failed {
    background-image: none;
    background-color: var(--common-target-fail);
    border-color: var(--common-target-fail);
    color: var(--common-target-hover-text-color);
}


